{
    "manifest_version": 3,
    "name": "Netflix Deserve",
    "version": "1.0",
    "description": "Pay money deserve the usage",
    "permissions": ["scripting"],
    "host_permissions": ["*://www.netflix.com/*"],
    "content_scripts": [
      {
        "matches": ["*://www.netflix.com/*"],
        "js": ["content.js"]
      }
    ]
  }